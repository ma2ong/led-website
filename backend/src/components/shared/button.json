{
  "collectionName": "components_shared_buttons",
  "info": {
    "displayName": "按钮",
    "description": "可点击的按钮组件"
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string"
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "primary",
        "secondary",
        "outline",
        "text",
        "success",
        "danger",
        "warning",
        "info"
      ],
      "default": "primary"
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "small",
        "medium",
        "large"
      ],
      "default": "medium"
    },
    "icon": {
      "type": "string"
    },
    "iconPosition": {
      "type": "enumeration",
      "enum": [
        "left",
        "right"
      ],
      "default": "left"
    },
    "target": {
      "type": "enumeration",
      "enum": [
        "_self",
        "_blank"
      ],
      "default": "_self"
    },
    "isDownload": {
      "type": "boolean",
      "default": false
    },
    "downloadFile": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false
    }
  }
}